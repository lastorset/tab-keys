{
  "name": "Tab keys",
  "short_name": "Tab keys",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Keyboard shortcuts for manipulating tabs",
  "icons": {
    "16":  "images/icons/rasterized/clone-tab-16.png",
    "48":  "images/icons/rasterized/clone-tab-48.png",
    "128":  "images/icons/rasterized/clone-tab-128.png"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "offline_enabled": true,
  "web_accessible_resources": [
  ],
  "commands": {
    "clone-tab": {
      "description": "Clone tab",
      "suggested_key": {
        "default": "Alt+C",
        "mac": "Alt+C"
      }
    }
  }
}
